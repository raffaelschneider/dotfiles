Config { font = "xft:Source Code Pro:style=Regular:size=9,FontAwesome:style=Regular"
       , additionalFonts = [ "xft:Iosevka Extended:style=Regular:size=9:antialias=true" ]
       , bgColor = "#2b303b"
       , fgColor = "#c0c5ce"
       , border = BottomB 2
       , borderColor = "#343d46"
       , position = Static { xpos = 0 , ypos = 0, width = 1600, height = 20}
       , lowerOnStart = True
       , hideOnStart = False
       , persistent = True
       , allDesktops = True
       , sepChar = "%"
       , alignSep = "}{"
       , template = " %StdinReader% }{ %mpd%  %wifi%  %vol% %bat%  %date%"
       , commands = [ Run Date "\xf073 <fn=1> %a %b %_d </fn> \xf017 <fn=1>%H:%M</fn> " "date" 10,
                      Run StdinReader,
                      Run Com "zsh" ["/home/zara/.xmobar/bat"] "bat" 30,
                      Run Com "zsh" ["/home/zara/.xmobar/brt"] "brt" 15,
                      Run Com "zsh" ["/home/zara/.xmobar/mpd"] "mpd" 5,
                      Run Com "zsh" ["/home/zara/.xmobar/volume"] "vol" 10,
                      Run Com "zsh" ["/home/zara/.xmobar/wifi"] "wifi" 15
                    ]
       }
